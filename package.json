{
  "name": "obsidian-gitlog-summary",
  "version": "1.2.0",
  "description": "Insert git log summary into your notes with branch status and date selection",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint src/",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "git",
    "git-log"
  ],
  "author": "takeshy",
  "license": "MIT",
  "devDependencies": {
    "@eslint/json": "^0.14.0",
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.2",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "obsidian": "^1.10.0",
    "tslib": "^2.4.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.49.0"
  },
  "dependencies": {
    "handlebars": "^4.7.8"
  }
}
